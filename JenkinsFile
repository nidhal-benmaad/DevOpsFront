pipeline {
    agent any
    stages {
         stage ('GIT') {
            steps {
               echo "Getting Project from Git";
                    git branch: "ghadaBranch",
                        url: "https://github.com/nidhal-benmaad/DevOpsFront.git",
                        credentialsId:"gitHub";


                   }
           tools {
                 nodejs 'NodeJS 18.5.0'
             }

             stages {
                 stage('Build') {
                     steps {
                         sh 'npm --version'
                         sh 'npm install'
                         // sh 'npm run build --prod'
                     }
                 }
                         }

    }
}
